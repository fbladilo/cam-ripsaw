apiVersion: migration.openshift.io/v1alpha1
kind: MigMigration
metadata:
  labels:
    controller-tools.k8s.io: "1.0"
  name: {{ cam_migration_name }}
  namespace: {{ cam_ns }}
spec:
  stage: false
{% if quiesce is defined %}
  quiescePods: {{ quiesce }}
{% endif %}
  migPlanRef:
    name: {{ cam_migration_plan_name }}
    namespace: {{ cam_ns }}
